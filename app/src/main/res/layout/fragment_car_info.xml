<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/yolov3cam_out"
        android:layout_width="match_parent"
        android:layout_height="450dp"
        android:layout_gravity="center_horizontal" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">
            <TextView
                android:id="@+id/crnn_out_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="车牌识别结果：" />
            <TextView
                android:id="@+id/logo_out_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="车标识别结果："  />
            <TextView
                android:id="@+id/type_out_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="车型识别结果：" />

            <TextView
                android:id="@+id/color1_out_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="车颜色识别结果：" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">
            <TextView
                android:id="@+id/crnn_out"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="" />
            <TextView
                android:id="@+id/logo_out"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=""  />
            <TextView
                android:id="@+id/type_out"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="" />

            <TextView
                android:id="@+id/color1_out"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="" />
        </LinearLayout>
    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">
            <TextView
                android:id="@+id/crnn_out_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="数据库车牌信息：" />
            <TextView
                android:id="@+id/logo_out_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="数据库车标信息："  />
            <TextView
                android:id="@+id/type_out_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="数据库车型信息：" />

            <TextView
                android:id="@+id/color1_out_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="数据库车颜色信息：" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="是否为套牌车："/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">
            <TextView
                android:id="@+id/crnn_database"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="" />
            <TextView
                android:id="@+id/logo_database"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=""  />
            <TextView
                android:id="@+id/type_database"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="" />

            <TextView
                android:id="@+id/color1_database"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>

            <TextView
                android:id="@+id/final_is"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>